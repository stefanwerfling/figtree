{
  "projects": [
    {
      "autoGenerate": true,
      "code": {
        "codeComment": true,
        "codeIndent": "    ",
        "createIndex": true,
        "createTypes": true,
        "schemaPrefix": "Schema"
      },
      "destinationClear": true,
      "destinationPath": "./schemas/src",
      "name": "Figtree",
      "scripts": {
        "after_generate": [
          {
            "script": "npm run compile",
            "path": "./schemas"
          }
        ]
      },
      "schemaPath": "./schemas/schemas.json"
    }
  ],
  "server": {
    "port": 5173
  }
}